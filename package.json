    "mysql2": "^3.9.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "nodeman": "^1.1.2",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
