    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.6.2",
    "mongoose": "^8.4.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.34.0",
    "concurrently": "^7.1.0",
    "cookie-session": "^2.0.0",
    "debug": "~2.6.9",
    "express-fileupload": "^1.3.1",
    "express-validator": "^6.14.2",
    "googleapis": "^111.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
